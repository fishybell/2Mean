{
  "name": "toomean",
  "version": "0.9.0",
  "description": "Angular 2, Nodejs, Express and Mongo stack",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/SquareHook/2Mean.git"
  },
  "scripts": {
    "test": "karma start config/karma.conf.js",
    "build": "webpack --config ./config/webpack/webpack.common.js --watch",
    "server": "if $NODE_DEBUG; then npm run server:debug; else npm run server:dev; fi",
    "server:dev": "node-dev ./server.js",
    "server:debug": "node-dev --inspect ./server.js",
    "watch-menus": "watch \"node watch-menus.js modules/**/client/config/menu.json > modules/core/client/config/menus.json\"",
    "start": "concurrently --kill-others --names \"webpack,node,config\" --prefix \"[{name}]\" --prefix-colors \"green,blue,red\" \"npm run build\" \"npm run server\" \"npm run watch-menus\"",
    "postinstall": "if [ ! -e ./config/private/cacert.pem ]; then npm run gen-cert; else echo \"TLS files already exist\"; fi",
    "gen-cert": "./init-ssl.sh",
    "lint:ts": "tslint -c ./config/tslint.json ./modules/*/client/*.ts ./modules/*/client/**/*.ts"
  },
  "keywords": [
    "Angular2",
    "Express",
    "Node",
    "Mongodb"
  ],
  "author": "Jason Bennett",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^4.2.4",
