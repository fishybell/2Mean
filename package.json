    "angular4-notifications": "^0.0.2",
    "argon2": "^0.15.0",
    "aws": "0.0.3-2",
    "aws-sdk": "^2.7.21",
    "body-parser": "^1.15.2",
    "bootstrap": "^4.0.0-alpha.5",
